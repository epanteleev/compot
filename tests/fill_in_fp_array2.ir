extern void @printDoubleArray(ptr, i64)

define i32 @main() {
entry:
  %retval = alloc i32
  %array  = alloc <f64 x 5>
  %i      = alloc f64
  store ptr %retval, i32 0
  store ptr %i, f64 0
  br label %forCond

forCond:
  %0 = load f64 %i
  %cmp = fcmp olt f64 %0, 5
  br u1 %cmp label %forBody, label %forEnd

forBody:
  %1 = load f64 %i
  %idx = fptosi f64 %1 to i64
  %arrayidx = gep f64, ptr %array, i64 %idx
  store ptr %arrayidx, f64 %1
  br label %forInc

forInc:
  %3 = load f64 %i
  %inc = add f64 %3, 1.0
  store ptr %i, f64 %inc
  br label %forCond

forEnd:
  %arraydecay = gep f64, ptr %array, i8 0
  call void @printDoubleArray(%arraydecay: ptr, 5: i64)
  %r = load i32 %retval
  ret i32 %r
}
