extern void printInt(u64)


define u64 main() {
entry:
        %1 = call i32 fib(8: i32)
	call void printInt(%1: u64)
        ret u64 0
}

define i32 fib(%0: i32) {
entry:
  %1 = stackalloc i32* 1
  %2 = stackalloc i32* 1
  store i32* %2, %0
  %3 = load i32 %2
  %4 = icmp ne i32 %3, 1
  br u1 %4 label L1, label L2

L1:
  %5 = load i32 %2
  %6 = icmp eq i32 %5, 0
  br u1 %6 label L2, label L3

L2:
  %7 = load i32 %2
  store i32* %1, %7
  br label L4

L3:
  %8 = load i32 %2
  %9 = sub i32 %8, 1
  %10 = call i32 fib(%9: i32)
  %11 = load i32 %2
  %12 = sub i32 %11, 2
  %13 = call i32 fib(%12: i32)
  %14 = add i32 %10, %13
  store i32* %1, %14
  br label L4

L4:
  %5 = load i32 %1
  ret i32 %5
}
