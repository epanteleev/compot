extern void @printDouble(f64)

define f32 @main() {
  %double = call f64 @f32_to_f64(-1: f32)
  call void @printDouble(%double: f64) br label %end

end:
  ret f32 0
}

;double f32_to_f64(float i) {
;    return i;
;}
define f64 @f32_to_f64(%i: f32) {
entry:
  %1 = alloc f32
  store ptr %1, f32 %i
  %2 = load f32 %1
  %3 = fpext f32 %2 to f64
  ret f64 %3
}
