extern void @printInt(i32)

define i32 @main() {
  %int = call i32 @i64_to_i32(-1: i64)
  call void @printInt(%int: i32)
  ret i32 0
}

;long i32_to_i64(int i) {
;    return i;
;}
define i32 @i64_to_i32(%i: i64) {
entry:
  %1 = alloc i64
  store i64* %1, %i
  %2 = load i64 %1
  %3 = trunc i64 %2 to i32
  ret i32 %3
}
