extern void @printInt(i32)

define u64 @main() {
entry:
  %1 = call i32 @discriminant(8: i32, 8: i32, 8: i32)
  call void @printInt(%1: i32)
  ret u64 0
}

define i32 @discriminant(%a:i32, %b:i32, %c:i32) {
entry:	
  %aAddr = alloc i32
  %bAddr = alloc i32
  %cAddr = alloc i32
  store i32* %aAddr, %a
  store i32* %bAddr, %b 
  store i32* %cAddr, %c 
  %0 = load i32 %bAddr
  %1 = load i32 %bAddr
  %mul = mul i32 %0, %1
  %2 = load i32 %aAddr
  %mul1 = mul i32 4, %2
  %3 = load i32 %cAddr
  %mul2 = mul i32 %mul1, %3
  %sub = sub i32 %mul, %mul2
  ret i32 %sub
}

