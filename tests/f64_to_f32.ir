extern void @printFloat(f32)

define i32 @main() {
  %float = call f32 @f64_to_f32(-1.0: f64)
  call void @printFloat(%float: f32)
  ret i32 0
}

;long f32_to_f64(float i) {
;    return i;
;}
define f32 @f64_to_f32(%i: f64) {
entry:
  %1 = alloc f64
  store ptr %1, f64 %i
  %2 = load f64 %1
  %3 = fptrunc f64 %2 to f32
  ret f32 %3
}
