define i32 @manyBranches(%val: i32) {
entry:
  %valAddr = alloc i32
  %ret     = alloc i32
  store ptr %valAddr, i32 %val
  store ptr %ret, i32 0
  %0 = load i32 %valAddr
  %cmp = icmp gt i32 %0, 3
  br u1 %cmp label %ifThen, label %ifElse

ifThen:
  %1 = load i32 %valAddr
  %cmp1 = icmp lt i32 %1, 10
  br u1 %cmp1 label %ifThen2, label %ifEnd

ifThen2:
  store ptr %ret, i32 7
  br label %ifEnd

ifEnd:
  br label %ifEnd7

ifElse:
  %2 = load i32 %valAddr
  %cmp3 = icmp gt i32 %2, 20
  br u1 %cmp3 label %ifThen4, label %ifElse5

ifThen4:
  store ptr %ret, i32 20
  br label %ifEnd6

ifElse5:
  store ptr %ret, i32 30
  br label %ifEnd6

ifEnd6:
  br label %ifEnd7

ifEnd7:
  %3 = load i32 %ret
  ret i32 %3
}
