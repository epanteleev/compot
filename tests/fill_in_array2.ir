extern void printIntArray(i32*, i32)

define i32 main() {
entry:
  %retval = alloc i32
  %array  = alloc <i32 x 5>
  %i      = alloc i32
  store i32* %retval, 0
  store i32* %i, 0
  br label forCond

forCond:
  %0 = load i32 %i
  %cmp = icmp slt i32 %0, 5
  br u1 %cmp label forBody, label forEnd

forBody:
  %1 = load i32 %i
  %arrayidx = gep i32 %array, i32 %1
  store i32* %arrayidx, %1
  br label forInc

forInc:
  %3 = load i32 %i
  %inc = add i32 %3, 1
  store i32* %i, %inc
  br label forCond

forEnd:
  %arraydecay = gep i32* %array, i32 00
  call void printIntArray(%arraydecay: i32*, 5: i32)
  ret i32 0
}
