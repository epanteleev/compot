extern void printIntArray(i32*, i32)

define u64 main() {
entry:
	%array = alloc [i32 x 5]
	%zero = gep i32* %array, i32 0
	store i32* %zero, 0
	%first = gep i32* %array, i32 1
	store i32* %first, 1
	%second = gep i32* %array, i32 2
        store i32* %second, 2
	%third = gep i32* %array, i32 3
        store i32* %third, 3
	%forth = gep i32* %array, i32 4
        store i32* %forth, 4
	%begin = gep i32* %array, i32 0
	call void printIntArray(%begin: i32*, 5: i32)
	ret i32 0
}


