extern void @printLong(i64)

define i32 @main() {
  %long = call i64 @i32_to_i64(-1: i32)
  call void @printLong(%long: i64)
  ret i32 0
}

;long i32_to_i64(int i) {
;    return i;
;}
define i64 @i32_to_i64(%i: i32) {
entry:
  %1 = alloc i32
  store ptr %1, i32 %i
  %2 = load i32 %1
  %3 = sext i32 %2 to i64
  ret i64 %3
}
