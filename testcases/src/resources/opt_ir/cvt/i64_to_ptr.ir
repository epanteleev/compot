
extern void @printInt(i32)

define i32 @main() {
  %adr = alloc i32
  store ptr %adr, i32 7
  %cvt = ptr2int ptr %adr to i64
  call void @print_by_adr(%cvt: i64)
  ret i32 0
}

;char f32_to_i8(float i) {
