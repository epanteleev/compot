extern void @printFloat(f32)

define i32 @main() {
  %float = call f32 @i8_to_f32(-1: i8)
  call void @printFloat(%float: f32)
  ret i32 0
}

;float i8_to_f32( int i) {
;    return (float)i;
;}
define f32 @i8_to_f32(%i: i8) {
entry:
  %1 = alloc i8
  store ptr %1, i8 %i
  %2 = load i8 %1
  %3 = int2fp i8 %2 to f32
  ret f32 %3
}
