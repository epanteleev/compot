extern void @printFloat(f32)

define i32 @main() {
  %float = call f32 @u32_to_f32(255: u32)
  call void @printFloat(%float: f32) br label %end
end:
  ret i32 0
}

;float u32_to_f32(unsigned int i) {
;    return (float)i;
;}
define f32 @u32_to_f32(%i: u32) {
entry:
  %1 = alloc u32
  store ptr %1, u32 %i
  %2 = load u32 %1
  %3 = int2fp u32 %2 to f32
  ret f32 %3
}
