extern void @printFloat(f32)

define i32 @main() {
  %float = call f32 @i32_to_f32(-1: i32)
  call void @printFloat(%float: f32)
  ret i32 0
}

;float i32_to_f32( int i) {
;    return (float)i;
;}
define f32 @i32_to_f32(%i: i32) {
entry:
  %1 = alloc i32
  store ptr %1, i32 %i
  %2 = load i32 %1
  %3 = int2fp i32 %2 to f32
  ret f32 %3
}
