extern void @printUByte(u8)

define i32 @main() {
  %char = call u8 @f32_to_u8(-1.0: f32)
  call void @printUByte(%char: u8) br label %end

end:
  ret i32 0
}

;unsigned char f32_to_u8(float i) {
;    return (unsigned char)i;
;}
define u8 @f32_to_u8(%i: f32) {
entry:
  %1 = alloc f32
  store ptr %1, f32 %i
  %2 = load f32 %1
  %3 = fp2int f32 %2 to u8
  ret u8 %3
}