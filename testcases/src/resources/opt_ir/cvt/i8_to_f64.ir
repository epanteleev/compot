extern void @printFloat(f64)

define i32 @main() {
  %float = call f64 @i8_to_f64(-1: i8)
  call void @printFloat(%float: f64)
  ret i32 0
}

;float i8_to_f64( int i) {
;    return (float)i;
;}
define f64 @i8_to_f64(%i: i8) {
entry:
  %1 = alloc i8
  store ptr %1, i8 %i
  %2 = load i8 %1
  %3 = int2fp i8 %2 to f64
  ret f64 %3
}
